{
    "name": "david-prv/quickyphp",
    "description": "A handmade php micro-framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "david-prv",
            "email": "66866223+david-prv@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "ext-http": "*",
      "ext-json": "*",
      "ext-apache": "*",
      "ext-mysqli": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9"
    },
    "autoload": {
        "classmap": [
            "quicky/",
            "quicky/core/",
            "quicky/http/",
            "quicky/storage/",
            "quicky/utils/",
            "quicky/views/",
            "quicky/interfaces",
            "quicky/middleware"
        ]
    }
}
